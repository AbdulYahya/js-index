{
  "name": "js-index",
  "displayName": "JS Index",
  "description": "Builds a JS/TS index out of files in the directory.",
  "icon": "icon.png",
  "version": "0.0.2",
  "publisher": "joostlubach",
  "author": {
    "name": "Joost Lubach",
    "publishername": "joostlubach"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joostlubach/js-index.git"
  },
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:js-index.buildIndex"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "js-index.buildIndex",
        "title": "Build index"
      }
    ],
    "keybindings": [
        {
            "command": "js-index.buildIndex",
            "key": "ctrl+k i",
            "mac": "cmd+k i"
        }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {}
}
